{
  "id": "classic-top",
  "name": "classic-top",
  "layer": "top",
  "position": "top",
  "margin-top": 7,
  "margin-left": 7,
  "margin-right": 7,

  "height": 22,
  "include": [
    "$HOME/.config/waybar/modules/defaults.json",
    "$HOME/.config/waybar/modules/media/config.json"
  ],
  "modules-left": ["wlr/workspaces", "hyprland/submap", "custom/media"],
  "modules-center": ["clock"],
  "modules-right": [
    "pulseaudio",
    "backlight",
    "custom/gpu-usage",
    "cpu",
    "memory",
    "network",
    "battery"
  ]
}
